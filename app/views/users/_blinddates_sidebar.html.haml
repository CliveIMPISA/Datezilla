- if user_signed_in?
	- if current_user.blind_date == "yes"
		.row{:style=>"border: 0px solid #ddd; "}
			.panel.panel-default
				.panel-heading
					=  "Suggessted Blind Dates"
				
				%ul.list-group
					-# @blinddatingusers.each do |user|
					- if !@blinddatingusers.blank? 
						%li.list-group-item
							= @blinddatingusers.username
							%br
	
							-# check is the user is already on a blind date by counting the number is a relationship of a blinddate exist or not
							-if current_user.blinddating.blank? == true
								- @showFollowing = false
								- @followValue = "Start blinddating"
								
							-else
								- @showFollowing = true
								- @followValue = "Stop blinddating"
								
							.col.s4
								= react_component 'Connect', {followValue: @followValue, showFollowing: @showFollowing, userId: current_user.id}